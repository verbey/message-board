<h1><%= title %></h1>
<a href="/new">
	<button>New Message</button>
</a>
<div class="messages">
	<% for (let i = 0; i < messages.length; i++) { %>
	<div class="message">
		<p><%= messages[i].text %></p>
		<p><%= messages[i].user %></p>
		<p><%= messages[i].added %></p>
		<a href=<%= /message/ + i %>>
			<button>View Message</button>
		</a>
	</div>
	<% } %>
</div>
